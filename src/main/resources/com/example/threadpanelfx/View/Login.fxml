<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane fx:id="m_pane" prefHeight="250.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.threadpanelfx.Controller.JavaFXController.ClientLoginController">
   <children>
      <VBox prefHeight="250.0" prefWidth="600.0">
         <children>
            <HBox prefHeight="250.0" prefWidth="600.0">
               <children>
                  <VBox alignment="CENTER" prefHeight="250.0" prefWidth="600.0" spacing="20.0">
                     <children>
                        <Label text="Введите ваш никнейм:" />
                        <TextField fx:id="m_nameTextField" />
                        <HBox alignment="CENTER" prefHeight="100.0" prefWidth="600.0" spacing="40">
                           <children>
                              <Button fx:id="m_setNameButton" mnemonicParsing="false" onAction="#OnSetName" prefWidth="150.0" text="Подтвердить имя" />
                              <Button fx:id="m_readyForGameButton" disable="true" mnemonicParsing="false" onAction="#OnReadyForGame" prefWidth="150.0" text="Готов к игре" />
                           </children>
                        </HBox>
                     </children>
                     <padding>
                        <Insets bottom="20.0" left="100.0" right="100.0" top="20.0" />
                     </padding></VBox>
               </children>
            </HBox>
         </children></VBox>
   </children>
</AnchorPane>
